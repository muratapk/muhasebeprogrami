@model muhasebeprogrami.Models.Unvan
@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>




<form method="post" action="Create">
    <div class="form-group row">
        <label for="text" class="col-4 col-form-label">Ünvanı</label>
        <div class="col-8">
            <input id="text" asp-for="Unvan_Adi" type="text" class="form-control">
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-4 col-8">
            <button name="submit" type="submit" class="btn btn-primary">Ekle</button>
        </div>
    </div>
</form>